<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.dpma.pumaz.control.TerminEditController">
   <children>
      <Label layoutX="33.0" layoutY="30.0" text="Terminname" />
      <TextField fx:id="terminNameField" layoutX="33.0" layoutY="51.0" prefHeight="31.0" prefWidth="166.0" />
      <DatePicker fx:id="datePickerVon" layoutX="33.0" layoutY="106.0" prefHeight="31.0" prefWidth="130.0" promptText="Von" />
      <DatePicker fx:id="datePickerBis" layoutX="197.0" layoutY="106.0" prefHeight="31.0" prefWidth="130.0" promptText="Bis" />
      <ButtonBar layoutX="14.0" layoutY="250.0" prefHeight="40.0" prefWidth="316.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleSubmit" text="Übernehmen" />
            <Button minWidth="0.0" mnemonicParsing="false" onAction="#handleExit" prefHeight="31.0" prefWidth="169.0" text="Abbrechen" />
        </buttons>
      </ButtonBar>
      <Button layoutX="343.0" layoutY="106.0" mnemonicParsing="false" text="Plus" />
      <ColorPicker fx:id="colorPicker" layoutX="33.0" layoutY="197.0" />
      <Label layoutX="33.0" layoutY="157.0" text="Farbe auswählen:" />
   </children>
</AnchorPane>
